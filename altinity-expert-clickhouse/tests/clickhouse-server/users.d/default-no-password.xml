<!--
  Optional ClickHouse user snippet for memory skill testing.
  Place under /etc/clickhouse-server/users.d/default-no-password.xml.
  This allows local dictionary SOURCE(CLICKHOUSE(...)) to connect as default without a password.
-->
<clickhouse>
    <users>
        <default>
            <password></password>
            <networks> <ip>::/0</ip> </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>
</clickhouse>
